    "  -- Calculate the total and average salary for each department.\n",
    "  \n",
    "  SELECT \n",
    "    department,\n",
    "    SUM(salary) AS total_salary,\n",
    "    AVG(salary) AS average_salary\n",
    "FROM employees\n",
    "GROUP BY department;\n"
